{
  "name": "@ngx-builders/pwa-local-storage",
  "version": "14.0.0",
  "description": "Efficient local storage module for Angular: simple API based on native localStorage API, but internally stored via the asynchronous IndexedDB API for performance, and wrapped in RxJS observables to be homogeneous with other Angular modules.",
  "license": "MIT",
  "keywords": [
    "angular",
    "localstorage",
    "local storage",
    "storage",
    "indexeddb",
    "angular13",
    "angular 13",
    "rxjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/santoshyadavdev/ngx-pwa-offline.git"
  },
  "homepage": "https://github.com/santoshyadavdev/ngx-pwa-offline",
  "bugs": {
    "url": "https://github.com/santoshyadavdev/ngx-pwa-offline/issues"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@angular/common": ">=14.0.0",
    "@angular/core": ">=14.0.0",
    "rxjs": ">=6.5.3"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "migrations": "./schematics/migration.json"
  },
  "module": "fesm2015/ngx-builders-pwa-local-storage.mjs",
  "es2020": "fesm2020/ngx-builders-pwa-local-storage.mjs",
  "esm2020": "esm2020/ngx-builders-pwa-local-storage.mjs",
  "fesm2020": "fesm2020/ngx-builders-pwa-local-storage.mjs",
  "fesm2015": "fesm2015/ngx-builders-pwa-local-storage.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/ngx-builders-pwa-local-storage.mjs",
      "es2020": "./fesm2020/ngx-builders-pwa-local-storage.mjs",
      "es2015": "./fesm2015/ngx-builders-pwa-local-storage.mjs",
      "node": "./fesm2015/ngx-builders-pwa-local-storage.mjs",
      "default": "./fesm2020/ngx-builders-pwa-local-storage.mjs"
    }
  },
  "sideEffects": false
}